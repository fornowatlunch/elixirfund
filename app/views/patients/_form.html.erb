<%= form_for(@patient, :as => :patient, :url => patients_path) do |f| %>
  <%= f.label :name %><br />
  <%= f.text_field :name %>
  <%= f.label :city %><br />
  <%= f.text_field :city %>
  <%= f.label :state %><br />
  <%= f.text_field :state %>
  <%= f.label :zip_code, "Zip Code" %><br />
  <%= f.text_field :zip_code %>
  <%= f.label :phone %><br />
  <%= f.text_field :phone %>
  <%= f.submit "Create Patient" %>
<% end %>
